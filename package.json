{
  "name": "meanstackjs-basic",
  "version": "0.1.0",
  "description": "We are building in this framework to make your life as a designer or developer easier. We believe in making this framework usable for User Experience Designers, Quality Assurance Engineers, Software Engineers, Frontend & Backend Developers & Full Stack Engineers...",
  "main": "server.js",
  "standard": {
    "globals": [
      "angular",
      "toastr",
      "moment",
      "self",
      "_",
      "describe",
      "it",
      "__dirname",
      "localStorage",
      "io",
      "expect",
      "inject",
      "beforeEach"
    ],
    "ignore": [
      "commands/template/"
    ]
  },
  "scripts": {
    "test": "npm run mocha && npm run karma && npm run e2e && npm run standard",
    "e2e": "node_modules/nightwatch/bin/nightwatch || nightwatch",
    "mocha": "node node_modules/mocha/bin/mocha tests/unit/mocha.test.js  ",
    "karma": "node node_modules/karma/bin/karma start tests/unit/karma.test.js || karma start tests/unit/karma.test.js ",
    "standard": "node node_modules/standard/bin/cmd.js || standard",
    "postinstall": "node node_modules/bower/bin/bower install || bower install",
    "start": " node server.js",
    "development": "export NODE_ENV=development || set NODE_ENV=development && node node_modules/nodemon/bin/nodemon.js server.js || nodemon.js server.js",
    "qa": "export NODE_ENV=test || set NODE_ENV=test && node server.js",
    "nightwatch": "export NODE_ENV=nightwatch || set NODE_ENV=nightwatch && node server.js",
    "production": "export NODE_ENV=production || set NODE_ENV=production && node server.js"
  },
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.15.2",
    "compression": "^1.6.2",
    "connect-mongo": "^1.3.2",
    "cookie-parser": "^1.4.3",
    "ejs": "^2.5.1",
    "express": "^4.14.0",
    "express-session": "^1.14.1",
    "express-validator": "^2.20.8",
    "jsonwebtoken": "^7.1.9",
    "less": "^2.7.1",
    "lodash": "^4.15.0",
    "method-override": "^2.3.6",
    "mongoose": "^4.5.10",
    "mongoose-timestamp": "^0.6.0",
    "mongoose-validator": "^1.2.5",
    "morgan": "^1.7.0",
    "multer": "^1.2.0",
    "node-sass": "^3.8.0",
    "nodemailer": "^2.5.0",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "pug": "^2.0.0-beta6",
    "run-auto": "^2.0.0",
    "serial-concat-files": "^1.0.0",
    "uglify-js": "^2.7.3",
    "uglifycss": "0.0.21"
  },
  "devDependencies": {
    "blessed": "^0.1.81",
    "bower": "^1.7.1",
    "chai": "^3.4.1",
    "glob": "^7.0.6",
    "inquirer": "^1.0.2",
    "karma": "^0.13.16",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.2.2",
    "karma-jasmine": "^0.3.6",
    "karma-mocha": "^1.0.1",
    "mocha": "^2.5.3",
    "multiline": "^1.0.2",
    "nightwatch": "^0.8.18",
    "nightwatch-html-reporter": "^1.0.4",
    "nodemon": "^1.8.1",
    "phantomjs-prebuilt": "^2.1.7",
    "standard": "^8.0.0",
    "supertest": "^1.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/greenpioneersolutions/meanstackjs-basic.git"
  },
  "keywords": [
    "mean",
    "stack",
    "msjs",
    "mongo",
    "express",
    "angular",
    "node"
  ],
  "author": "Green Pioneer <info@greenpioneersolutions.com>",
  "contributors": "https://github.com/greenpioneersolutions/meanstackjs-basic/graphs/contributors",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/greenpioneersolutions/meanstackjs-basic/issues"
  },
  "homepage": "https://github.com/greenpioneersolutions/meanstackjs-basic#readme"
}
